{
  "grammars": [
    {
      "name": "ql-dbscheme",
      "camelcase": "QlDbscheme",
      "scope": "source.dbscheme",
      "path": ".",
      "file-types": [
        "dbscheme"
      ],
      "injection-regex": "dbscheme"
    }
  ],
  "metadata": {
    "version": "0.23.0",
    "license": "MIT",
    "description": "QL DBScheme grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-ql-dbscheme"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
